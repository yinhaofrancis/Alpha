{
  "name": "Ham",
  "version": "0.2.25",
  "summary": "francis private Module",
  "description": "francis private Module with objective-C",
  "homepage": "https://github.com/yinhaofrancis/Ham",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yinhaofrancis": "1833918721@qq.com"
  },
  "source": {
    "git": "https://github.com/yinhaofrancis/Ham.git",
    "tag": "0.2.25"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Module",
  "subspecs": [
    {
      "name": "Module",
      "source_files": "Ham/Classes/Module/**/*",
      "public_header_files": "Ham/Classes/Module/**/*.h"
    },
    {
      "name": "Render",
      "platforms": {
        "ios": null
      },
      "source_files": "Ham/Classes/Render/**/*",
      "public_header_files": "Ham/Classes/Render/**/*.h",
      "user_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HMRENDERMODULE=1"
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HMRENDERMODULE=1"
      },
      "dependencies": {
        "Ham/Module": [

        ]
      }
    },
    {
      "name": "iOS",
      "platforms": {
        "ios": null
      },
      "source_files": "Ham/Classes/iOS/**/*",
      "public_header_files": "Ham/Classes/iOS/**/*.h",
      "user_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HMIOS=1"
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HMIOS=1"
      },
      "dependencies": {
        "Ham/Module": [

        ]
      }
    },
    {
      "name": "H5",
      "platforms": {
        "ios": null
      },
      "source_files": "Ham/Classes/H5/**/*",
      "public_header_files": "Ham/Classes/H5/**/*.h",
      "user_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HMH5MODULE=1"
      },
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HMH5MODULE=1"
      },
      "dependencies": {
        "Ham/iOS": [

        ]
      }
    }
  ]
}
